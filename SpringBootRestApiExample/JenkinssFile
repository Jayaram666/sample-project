pipeline {
    agent any

    stages {
        stage ('Clone') {
            steps {
                git branch: 'master', url: "https://github.com/Jayaram666/sample-project.git"
            }
        }
        stage('Build') {
        withMaven(maven: 'M2_HOME') {
        dir('SpringBootRestApiExample') {
        bt 'mvn clean package'
      }
    }
  }
  stage('Archive') {
         steps {
           dir ('SpringBootRestApiExample/target') {
           archive '*.jar'
         }  
      } 
   } 
   }